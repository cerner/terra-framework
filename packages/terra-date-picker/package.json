  },
  "homepage": "https://github.com/cerner/terra-framework#readme",
  "peerDependencies": {
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-intl": "^2.8.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "focus-trap-react": "^6.0.0",
    "keycode-js": "^2.0.1",
    "moment": "^2.21.0",
    "prop-types": "^15.5.8",
    "react-onclickoutside": "^6.7.1",
    "react-portal": "^4.1.2",
    "terra-breakpoints": "^2.0.0",
