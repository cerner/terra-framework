{
  "name": "terra-framework-site",
  "version": "0.0.0",
  "description": "Documentation Site for Functional Verification",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerner/terra-framework.git"
  },
  "author": "Cerner Corporation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cerner/terra-framework/issues"
  },
  "homepage": "https://github.com/cerner/terra-framework#readme",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "compile": "npm run compile:clean && npm run compile:build",
    "compile:clean": "rimraf lib",
    "compile:build": "babel src --out-dir lib --copy-files",
    "compile:heroku": "webpack --config webpack.prod.config --progress",
    "compile:prod": "webpack --config webpack.prod.config -p",
    "deploy": "npm run compile:prod && gh-pages -d build",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .js,.jsx . --ignore-path ../../.eslintignore",
    "test": "npm run test:jest",
    "test:jest": "jest node tests/jest/* --config ../../jestconfig.json"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "postcss": "^6.0.9",
    "prop-types": "^15.5.8",
    "react-intl": "^2.3.0",
    "react-router": "^3.0.5",
    "terra-base": "^2.6.1",
    "terra-collapsible-menu-view": "^1.1.1",
    "terra-content-container": "^1.11.1",
    "terra-i18n": "^1.10.1",
    "terra-i18n-plugin": "^1.7.0",
    "terra-icon": "^1.12.1",
    "terra-list": "^1.12.1",
    "terra-markdown": "^1.9.1",
    "terra-props-table": "^1.10.1",
    "terra-slide-panel": "^1.11.1",
    "terra-theme-provider": "^1.6.1"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.23.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.30.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.2",
    "postcss-custom-properties": "^6.0.1",
    "postcss-loader": "^2.0.6",
    "postcss-rtl": "^1.1.2",
    "raw-loader": "^0.5.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "2.7.1"
  }
}
